               <bpel:extensionAssignOperation>
                  <ext2:script expressionLanguage="urn:active-endpoints:expression-language:xquery1.0">let $processName := abx:getProcessName() 
let $statementId := data($gDataAccessRequest/das:sqlStatement/@statementId)
let $dasType := switch (true())
 case (exists($gDataAccessRequest/das:sqlStatement/das:parameterBatch)) return concat("Batch Size:",count( $gDataAccessRequest/das:sqlStatement/das:parameterBatch ))
 case (exists($gDataAccessRequest/das:sqlStatement/das:statement)) return "Single Statement"
 case (exists($gDataAccessRequest/das:sqlStatement/das:procedure)) return "Stored Procedure"
 default return ()

let $error := abx:getFaultString()
 
let $result := "FAILED"

let $titleparts := ($processName, $statementId, $dasType, $result, $error)
return
abx:setProcessTitle(substring(string-join($titleparts," "),1,255))</ext2:script>
               </bpel:extensionAssignOperation>
